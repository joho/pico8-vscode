{
    "function": {
        "prefix": "function",
        "body": [
            "function $1($2)",
            "\t$2",
            "end"
        ]
    },
    "for-range": {
        "prefix": "for-range",
        "body": [
            "for i=1,$1 do",
            "\t$2",
            "end"
        ]
    },
    "for-array": {
        "prefix": "for-array",
        "body": [
            "for el in all($1) do",
            "\t$2",
            "end"
        ]
    },
    "for-table": {
        "prefix": "for-table",
        "body": [
            "for key,val in pairs($1) do",
            "\t$2",
            "end"
        ]
    }
}